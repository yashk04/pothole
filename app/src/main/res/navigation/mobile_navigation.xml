<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_image">

    <fragment
        android:id="@+id/navigation_image"
        android:name="com.example.pothole.Citizen.image.ImageFragment"
        android:label="@string/title_image"
        tools:layout="@layout/fragment_image" />

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.example.pothole.Citizen.maps_citizen.MapsFragment"
        android:label="@string/title_Maps"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.pothole.Citizen.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.pothole.Citizen.account.AccountFragment"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_account" />

</navigation>